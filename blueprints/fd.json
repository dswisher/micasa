{
    "package_names":
    {
        "apt-get":
        {
            "ubuntu24": "fd-find"
        }
    },
    "version_check":
    {
        "executable":
        {
            "default": "fd",
            "ubuntu24": "fdfind"
        },
        "arg": "--version",
        "regex": "fd(?:find)? ([0-9]+\\.[0-9]+\\.[0-9]+)"
    }
}
