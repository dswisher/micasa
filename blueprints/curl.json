{
    "package_names":
    {
        "brew": "curl",
        "apt-get":
        {
            "ubuntu22": "curl",
            "ubuntu24": "curl"
        }
    },
    "version_check":
    {
        "executable": "curl",
        "arg": "--version",
        "regex": "curl ([0-9]+\\.[0-9]+\\.[0-9]+)"
    }
}
