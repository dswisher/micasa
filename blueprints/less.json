{
    "package_names":
    {
        "apt-get":
        {
            "debian12": "less",
            "ubuntu22": "less",
            "ubuntu24": "less"
        },
        "dnf":
        {
            "amazonlinux2023": "less"
        }
    },
    "version_check":
    {
        "executable": "less",
        "arg": "--version",
        "regex": "less ([0-9]+(?:\\.[0-9]+)?)"
    }
}
